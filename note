	MODEL PERANAN BIASA
{
	grup: 'pendaftaran'
	admin: 'admin_pendaftaran@rspb.com' # Harus PNS
	menus: ['pendaftaran', 'rawat']
	petugas: [
		'petugas1_pendaftaran@rspb.com'
		'petugas2_pendaftaran@rspb.com'
		...
		'petugas12_pendaftara@rspb.com'
	]
}

	MODEL PERANAN RAWAT BERSUB
{
	grup: 'rawat_jalan'
	menus: ['rawat_jalan', 'apotik']
	subs: [
		sub: 'mata'
		petugas: [
			'petugas1@rspb.com'
			'petugas2@rspb.com'
		]
		# ...lanjutkan dengan 11 poli lainnya
	]
}

	MODEL PERANAN RAWAT INAP
{
	grup: 'inap'
	admin: 'admin_inap@rspb.com' # kepala instalasi
	subs: [
		sub: 'pda'
		adsub: 'admin_pda@rspb.com' # kepala ruangan
		petugas: [
			'pda1@rspb.com' # bisa pns / honorer
			...
			'pda12@rspb.com'
		]
		
		# lanjutkan ..
		'bedah'
		'kebidanan'
		'lantai3'
	]
	
}
# kalau ada kali pakai subs
pendaftaran 1 admin 12 petugas menus['pendaftaran', 'rawat']
pembayaran 1 admin 7 petugas menus['pembayaran']
jalan 1 admin(poli) 2x12 petugas(poli) menus['rawat', 'apotik']

inap 4 admin(ruangan) 4x7 petugas(ruangan) menus['rawat', 'apotik']
igd 1 admin 10 petugas menus['rawat', 'apotik']
labor 1 admin 10 petugas menus['labor']
radiologi 1 admin 10 petugas menus['radio']
apotek+farmasi 1 admin 2x10 petugas menus['apotik', 'farmasi']
rekam_medik 1 admin menus['rekam', 'pendaftaran']
admisi 1 admin 3 petugas menus['admisi']
